    },
    "devDependencies": {
        "@types/cheerio": "0.22.22",
        "@types/express": "4.17.8",
        "@types/express-promise-router": "3.0.0",
        "@types/node": "14.11.2",
        "@types/node-fetch": "2.5.7",
        "@types/promise-timeout": "1.3.0",
        "@types/puppeteer": "^3.0.2",
        "tslint": "^6.1.3",
        "typescript": "^4.0.3"
    }
